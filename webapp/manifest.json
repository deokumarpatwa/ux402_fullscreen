{
  "_version": "1.59.0",
  "sap.app": {
    "id": "studentdk.com.sap.training.ux402.fullscreen.ux402fullscreen",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.14.2",
      "toolsId": "635e5361-ff67-4d3e-99fe-48dfea8e89a5"
    },
    "dataSources": {
      "mainService": {
        "uri": "/here/goes/your/serviceurl/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.126.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "studentdk.com.sap.training.ux402.fullscreen.ux402fullscreen.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "studentdk.com.sap.training.ux402.fullscreen.ux402fullscreen.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition":"slide",
        "clearControlAggregation": false,
        "bypassed": {
          "target":["notFound"]
        }
      },
      "routes": [
        {
          "name": "overview",
          "pattern": "",
          "titleTarget": "",
          "greedy": false,
          "target": [
            "overview"
          ]
        },
        {
          "name": "flights",
          "pattern": "carriers/{carrid}",
          "titleTarget": "",
          "greedy": false,
          "target": ["flights"]
        }
      ],
      "targets": {
        "overview": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Carrier",
          "viewName": "Carrier",
          "viewLevel": 1
        },
        "flights": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "Flights",
          "viewName": "Flights",
          "viewLevel": 2
        },      
        "notFound": {
          "viewId": "NotFound",
          "viewName": "NotFound",
          "transition": "show"
        }
      }
    },
    "rootView": {
      "viewName": "studentdk.com.sap.training.ux402.fullscreen.ux402fullscreen.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
